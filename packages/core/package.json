{
  "name": "@zk-payroll/core",
  "version": "0.1.0",
  "description": "Core SDK for interacting with ZK Payroll contracts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc",
    "test": "jest",
    "clean": "rimraf dist",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" \"tests/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "docs:build": "typedoc"
  },
  "dependencies": {
    "@stellar/stellar-sdk": "^14.5.0",
    "axios": "^1.7.0",
    "snarkjs": "^0.7.5",
    "stellar-sdk": "^11.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.56.1",
    "eslint": "^10.0.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "jest": "^29.0.0",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.3",
    "ts-jest": "^29.0.0",
    "typedoc": "^0.28.17",
    "typescript": "^5.0.0"
  }
}
